<p id="notice"><%= notice %></p>

<h1>Detalle Orden Presupuestos</h1>

<table>
  <thead>
    <tr>
      <th>Orden trabajo</th>
      <th>Presupuesto</th>
      <th>Repuesto servicio</th>
      <th>Cantidad</th>
      <th>Precio unitario</th>
      <th>Subtotal</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalle_orden_presupuestos.each do |detalle_orden_presupuesto| %>
      <tr>
        <td><%= detalle_orden_presupuesto.orden_trabajo_id %></td>
        <td><%= detalle_orden_presupuesto.presupuesto_id %></td>
        <td><%= detalle_orden_presupuesto.repuesto_servicio_id %></td>
        <td><%= detalle_orden_presupuesto.cantidad %></td>
        <td><%= detalle_orden_presupuesto.precio_unitario %></td>
        <td><%= detalle_orden_presupuesto.subtotal %></td>
        <td><%= detalle_orden_presupuesto.total %></td>
        <td><%= link_to 'Show', detalle_orden_presupuesto %></td>
        <td><%= link_to 'Edit', edit_detalle_orden_presupuesto_path(detalle_orden_presupuesto) %></td>
        <td><%= link_to 'Destroy', detalle_orden_presupuesto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

